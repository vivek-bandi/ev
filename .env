# Frontend Environment Variables

# API Configuration
VITE_API_BASE_URL="http://localhost:5000"
VITE_API_TIMEOUT=30000

# App Configuration
VITE_APP_NAME="EV Vehicle Store"
VITE_APP_DESCRIPTION="Premium Electric Vehicles for a Sustainable Future"
VITE_APP_URL="http://localhost:8080"

# Authentication
VITE_JWT_STORAGE_KEY="ev_auth_token"

# Image Upload
VITE_MAX_IMAGE_SIZE=5242880 # 5MB in bytes
VITE_ALLOWED_IMAGE_TYPES="image/jpeg,image/png,image/webp"

# Feature Flags
VITE_ENABLE_TEST_DRIVES=true
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_NOTIFICATIONS=true

# Development Tools
VITE_ENABLE_QUERY_DEBUGGER=false
VITE_ENABLE_API_MOCKING=false
